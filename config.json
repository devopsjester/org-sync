{
    "name": "org-sync-app",
    "version": "0.1.0",
    "description": "Syncs GitHub orgs and their repos",
    "author": "Assaf Stone, @devopsjester",
    "license": "MIT",
    "settings": {
        "orgsToSync": [
            {
                "features": { "syncRepos": true, "syncPullRequests": false },
                "source": "org-sync-source",
                "shadows": ["org-sync-target"],
                "whitelist": ["*"],
                "exceptions": ["org-sync"]
            }
        ]
    }
}